---
import '~/styles/styles.css';
import SEO from './SEO.astro';

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<!-- Remove the hard-coded dark mode and implement your own if you're into that sort of thing -->
<html lang="en" data-theme="dark">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" type="image/svg+xml" href="images/icon.ico" />
    <meta name="generator" content={Astro.generator} />
    <meta name="theme-color" content="#9EA3FA" />
<meta name="google-site-verification" content="IEP8CtRyxmhH5TfGyg0pgX601G4IOuhhfydVUFV_GwY" />
<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-4HFX429PWK"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-4HFX429PWK');
</script>   
<SEO />

    <title>{title}</title>

    <!-- preload fonts -->

    <link
      rel="preload"
      href="/fonts/SilkSerif-ExtraLight.woff2"
      as="font"
      type="font/woff2"
      crossorigin=""
    />
    <link
      rel="preload"
      href="/fonts/SilkSerif-Light.woff2"
      as="font"
      type="font/woff2"
      crossorigin=""
    />
  </head>
  <body>
    <div class="tiledBackground">
      <div id="topblock"></div>
      <h1 style="text-align:center;"><a class="lg" href="/">SUBZERO FRIDGE REPAIR</a></h1>
